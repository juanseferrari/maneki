{
  "name": "maneki",
  "version": "1.0.0",
  "description": "File upload application with Supabase Storage",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "npm run build:css && nodemon server-dev.js",
    "dev:auth": "nodemon server-supabase.js",
    "build:css": "npx tailwindcss -i ./public/css/tailwind-input.css -o ./public/css/output.css",
    "watch:css": "npx tailwindcss -i ./public/css/tailwind-input.css -o ./public/css/output.css --watch",
    "test": "jest --coverage",
    "test:watch": "jest --watch",
    "test:unit": "jest tests/unit",
    "test:integration": "jest tests/integration",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix"
  },
  "keywords": [
    "file-upload",
    "supabase",
    "express",
    "ejs"
  ],
  "author": "",
  "license": "ISC",
  "engines": {
    "node": "18.x",
    "npm": "9.x"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.30.1",
    "@supabase/supabase-js": "^2.39.7",
    "axios": "^1.13.2",
    "connect-pg-simple": "^9.0.1",
    "csv-parse": "^5.5.5",
    "dotenv": "^16.4.5",
    "ejs": "^3.1.9",
    "express": "^4.18.2",
    "express-session": "^1.17.3",
    "jwa": "^2.0.1",
    "multer": "^1.4.5-lts.1",
    "node-cron": "^3.0.3",
    "passport": "^0.7.0",
    "passport-google-oauth20": "^2.0.0",
    "pdf-parse": "^1.1.1",
    "pg": "^8.11.3",
    "safer-buffer": "^2.1.2",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@eslint/js": "^9.0.0",
    "autoprefixer": "^10.4.24",
    "daisyui": "^5.5.18",
    "eslint": "^9.0.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-jest": "^28.0.0",
    "globals": "^15.0.0",
    "jest": "^29.7.0",
    "nodemon": "^3.0.3",
    "postcss": "^8.5.6",
    "shadcn": "^3.8.5",
    "supertest": "^6.3.4",
    "tailwindcss": "^3.4.19"
  }
}
